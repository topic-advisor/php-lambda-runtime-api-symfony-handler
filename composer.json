{
    "name": "topic-advisor/php-lambda-runtime-api-symfony-handler",
    "description": "Symfony handler for the topic-advisor/php-lambda-runtime-api library",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "require": {
        "php": "^7.3",
        "php-pm/httpkernel-adapter": "^1.0",
        "symfony/dependency-injection": "^4.2|^5.0",
        "symfony/http-foundation": "^3.4|^4.0|^5.0",
        "symfony/http-kernel": "^3.4|^4.0|^5.0",
        "symfony/psr-http-message-bridge": "^1.2",
        "topic-advisor/php-lambda-runtime-api": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "TopicAdvisor\\Lambda\\Symfony\\Handler\\": "src/Lambda/Symfony/Handler/"
        }
    }
}
